package com.myapp.basics;

public class JavaStrings {

	public static void main(String[] args) {
		String str="hello..!";
		
		System.out.println(str.length());
		
		// Lower Case:
		str.toLowerCase();
		System.out.println(str);
		
		// Upper Case:
		str.toUpperCase();
		System.out.println(str);

	}

}
